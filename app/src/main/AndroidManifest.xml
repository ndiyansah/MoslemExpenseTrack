<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.MET.salman.MoeslemExpenseTracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StudentForm"
            android:label="Student Form" />
        <activity
            android:name=".NewTransaction"
            android:label="@string/title_activity_new_transaction"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".EditDeleteForm"
            android:label="@string/title_activity_edit_delete_form"
            android:theme="@style/AppTheme" />
        <activity android:name=".EmployeeForm" />
    </application>

</manifest>
